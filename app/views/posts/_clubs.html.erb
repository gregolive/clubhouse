<div class="column is-one-quarter">
  <nav class="panel">
    <p class="panel-heading">Clubs</p>
    <% Post::CLUBS.each_with_index do |club, i| %>
      <%= link_to @post, class: "panel-block icon-text" do %>
        <div class="icon-text small-gap">
          <span class="panel-icon">
            <span class="icon"><i class="<%= Post::CLUB_IMG[i] %>" aria-hidden="true"></i></span>
          </span>
          c/<%= club %>
        </div>
      <% end %>
    <% end %>
  </nav>
</div>